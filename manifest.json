{
  "manifest_version": 2,
  "name": "OLX Search (UA)",
  "description": "www.olx.ua search",
  "version": "1.0",
  "author": "Pavlo Samko <bulldozerbsg@gmail.com>",
  "homepage_url": "https://github.com/bulldozerbsg/olx-search",
  "icons": {
    "48": "icons/icon.svg",
    "96": "icons/icon.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "OLX (UA)",
      "search_url":"https://www.olx.ua/list/?q={searchTerms}",
      "keyword": "@olx",
      "favicon_url": "https://www.olx.ua/favicon.ico",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
